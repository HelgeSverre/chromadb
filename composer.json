{
    "name": "helgesverre/chromadb",
    "description": "PHP Client for the Chromadb Rest API",
    "keywords": [
        "laravel",
        "vectorstore",
        "vector",
        "database",
        "chromadb",
        "sdk",
        "php"
    ],
    "homepage": "https://github.com/helgesverre/chromadb",
    "license": "MIT",
    "authors": [
        {
            "name": "Helge Sverre",
            "email": "helge.sverre@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "saloonphp/saloon": "^3.0",
        "saloonphp/laravel-plugin": "^v3.0.0",
        "spatie/laravel-data": "^4.0",
        "spatie/laravel-package-tools": "^1.92.1"
    },
    "require-dev": {
        "larastan/larastan": "^2.0.1",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8",
        "orchestra/testbench": "^8.8 || ^9.0 || ^10.0",
        "pestphp/pest": "^2.20",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/ray": "^1.42"
    },
    "autoload": {
        "psr-4": {
            "HelgeSverre\\Chromadb\\": "src/"
        },
        "files": [
            "src/Embeddings/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HelgeSverre\\Chromadb\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test:unit": "vendor/bin/pest --testsuite=Unit",
        "test:feature": "vendor/bin/pest --testsuite=Feature",
        "test:integration": "vendor/bin/pest --testsuite=Integration",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint src tests config"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HelgeSverre\\Chromadb\\ChromadbServiceProvider"
            ],
            "aliases": {
                "Chromadb": "HelgeSverre\\Chromadb\\Facades\\Chromadb"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
